<% include partials/header %>

<% var results = dataObj["Search"]; %>
<div id="resultbox">
    <% results.forEach(function(movie) { %>
        <h1> <%= movie["Title"] %></h1>
        <p>Year: <%= movie["Year"] %></p>
        <% if (movie["Poster"].match(/^http/g)) { %>
            <a href="/search/<%= movie["imdbID"] %>" target=".blank"><img src= <%= movie["Poster"]%> ></a>
        <% } %>
        <hr>
    <% }); %>
</div>

<% include partials/footer %>